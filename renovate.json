{
  "extends": [
    "github>FantasticFiasco/renovate-config"
  ],
  "packageRules": [
    {
      "packagePatterns": ["^@aws-cdk", "^aws-cdk"],
      "groupName": "AWS CDK packages"
    },
    {
      "datasources": ["nuget"],
      "packagePatterns": ["^AWSSDK."],
      "versioning": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)\\.(?<prerelease>.*)$"
    }
  ]
}
