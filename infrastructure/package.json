{
  "name": "signature-version-4-infrastructure",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "clean": "rimraf --glob \"./cdk.out\" \"./+(bin|lib)/**/*.+(d.ts|js|js.map)\"",
    "build": "npm run clean && tsc",
    "deploy": "npm run build && cdk deploy --app ./bin/infrastructure.js --all",
    "destroy": "npm run build && cdk destroy",
    "lint": "eslint --ext .ts --max-warnings=0 . && prettier --check .",
    "lint:fix": "eslint --ext .ts --fix . && prettier --write ."
  },
  "dependencies": {
    "aws-cdk-lib": "2.131.0",
    "constructs": "10.1.175",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.137",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "typescript": "5.4.5"
  },
  "resolutions": {
    "ansi-regex": ">=5.0.1",
    "bl": ">=4.0.3",
    "lodash": ">=4.17.13",
    "netmask": ">=2.0.1",
    "pac-resolver": ">=5.0.0",
    "vm2": ">=3.9.4"
  }
}
