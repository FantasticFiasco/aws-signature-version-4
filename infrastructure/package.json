  },
  "scripts": {
    "clean": "rimraf \"./cdk.out\"; rimraf \"./+(bin|lib|resources)/**/*.+(d.ts|js|js.map)\"",
    "prebuild": "yarn clean",
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "predeploy": "yarn build",
    "deploy": "cdk deploy",
    "predestroy": "yarn build",
    "destroy": "cdk destroy",
    "lint": "eslint --ext .ts --max-warnings=0 . && prettier --check .",
    "lint:fix": "eslint --ext .ts --fix . && prettier --write ."
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.97.0",
    "@aws-cdk/aws-cloudformation": "1.97.0",
    "@aws-cdk/aws-iam": "1.97.0",
    "@aws-cdk/aws-lambda": "1.97.0",
    "@aws-cdk/aws-s3": "1.97.0",
    "@aws-cdk/aws-s3-deployment": "1.97.0",
    "@aws-cdk/core": "1.97.0",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "aws-cdk": "1.97.0",
    "eslint": "7.23.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.19",
    "typescript": "4.2.4"
  },
  "resolutions": {
    "bl": ">=4.0.3",
