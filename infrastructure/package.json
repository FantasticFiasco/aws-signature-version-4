    "@aws-cdk/aws-cloudformation": "1.4.0",
    "@aws-cdk/aws-iam": "1.4.0",
    "@aws-cdk/aws-lambda": "1.4.0",
    "@aws-cdk/aws-s3": "1.4.0",
    "@aws-cdk/core": "1.4.0",
    "source-map-support": "0.5.13"
  },
  "devDependencies": {
    "@types/node": "8.10.51",
    "aws-cdk": "1.4.0",
    "rimraf": "3.0.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "lodash": ">=4.17.13"
  }
}
